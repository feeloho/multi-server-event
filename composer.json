{
    "name": "feeloho/multi-server-event",
    "description": "Laravel分布式服务器中防止事件多次触发加锁机制，需配合memcache或redis",
    "keywords": [
        "multi server"
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": ">=5.4 <=5.5"
    },
    "autoload": {
        "psr-4": {
            "Feeloho\\MultiServerEvent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feeloho\\MultiServerEvent\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "orchestra/testbench": "^3.4"
    }
}
